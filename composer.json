{
    "name": "th3mouk/yahoo-weather-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle that include Yahoo Weather API and archive datas in database.",
    "keywords": [
        "weather", "forecasts", "yahoo", "symfony", "bundle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy Marodon",
            "email": "marodon.jeremy@gmail.com"
        }
    ],
    "require": {
        "php": "^5.4|^7.0",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/http-kernel": "~2.3",
        "twig/twig": "^1.12",
        "th3mouk/yahoo-weather-api": "^1.0@dev",
        "gedmo/doctrine-extensions": "^2.3.12"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~2.7|~3.0",
        "fabpot/php-cs-fixer": "^0.5|^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Th3Mouk\\YahooWeatherBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
